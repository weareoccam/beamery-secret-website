---
layout: false
directory_index: false
---

<% pages = sitemap.resources.find_all{ |r| r.source_file.match(/\.html/) } %>
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<% pages.each do |page| %>
  <% next if page.data.exclude_from_sitemap %>
  <url>
    <loc>https://grantharr.is<%= page.destination_path.gsub('index.html', '') %></loc>
    <lastmod><%= Date.today.to_time.iso8601 %></lastmod>
    <priority><%= page.data.sitemap_priority || 0.5 %></priority>
  </url>
<% end %>
</urlset>