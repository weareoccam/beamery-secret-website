    "filesize": "^3.3.0",
    "lodash": "^4.16.4",
    "mkdirp": "^0.5.1",
    "opener": "^1.4.2"
  },
  "devDependencies": {
    "babel-core": "6.18.0",
    "babel-plugin-transform-runtime": "6.15.0",
